{
  "name": "function-plot",
  "version": "2.0.0-0",
  "description": "A simple 2d function plotter powered by d3",
  "license": "MIT",
  "author": "Mauricio Poppe <mauricio.poppe@gmail.com>",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "exports": {
    "umd": "./dist/function-plot.js",
    "default": "./dist/index.js"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "npx rimraf dist && npm run build:typescript && npm run build:webpack",
    "build:webpack": "NODE_ENV=production webpack",
    "build:typescript": "tsc && copy samplers\\*.mjs dist\\samplers\\",
    "start": "webpack-dev-server"
  },
  "engines": {
    "node": ">=12"
  },
  "files": [
    "/dist"
  ],
  "standard": {
    "ignore": [
      "dist/"
    ]
  },
  "dependencies": {
    "built-in-math-eval": "^0.3.0",
    "d3-axis": "^3.0.0",
    "d3-color": "^3.1.0",
    "d3-format": "^3.1.0",
    "d3-interpolate": "^3.0.1",
    "d3-scale": "^4.0.2",
    "d3-selection": "^3.0.0",
    "d3-shape": "^3.1.0",
    "d3-zoom": "^3.0.0",
    "events": "^3.3.0",
    "interval-arithmetic-eval": "^0.5.1",
    "web-worker": "^1.3.0"
  },
  "devDependencies": {
    "ts-loader": "^9.3.1",
    "typescript": "^4.8.3",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.11.0"
  }
}
